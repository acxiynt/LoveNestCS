{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Debug",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/LNCS.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "Release",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/LNCS.csproj",
                "-c",
                "Release"
            ],
            "problemMatcher": "$msCompile"
        }        
    ]
}